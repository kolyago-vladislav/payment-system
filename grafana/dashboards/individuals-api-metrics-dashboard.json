{
  "annotations": {
    "list": []
  },
  "description": "HTTP endpoints metrics with breakdown by URI and method",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "requests",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "targets": [
        {
          "expr": "sum by (uri, method) (http_server_requests_seconds_count{error=\"none\"})",
          "legendFormat": "{{method}} {{uri}}",
          "refId": "A"
        }
      ],
      "title": "Requests Count per Endpoint",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "id": 2,
      "targets": [
        {
          "expr": "sum by (uri, method) (http_server_requests_seconds_sum{error=\"none\"}) / sum by (uri, method) (http_server_requests_seconds_count{error=\"none\"})",
          "legendFormat": "{{method}} {{uri}}",
          "refId": "A"
        }
      ],
      "title": "Average Request Duration per Endpoint",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "id": 3,
      "targets": [
        {
          "expr": "sum by (error, outcome) (http_server_requests_seconds_count{outcome != \"SUCCESS\"} or http_server_requests_seconds_count{error != \"none\"})",
          "legendFormat": "{{method}} {{uri}}",
          "refId": "A"
        }
      ],
      "title": "Application Errors",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["http", "endpoints", "metrics"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "HTTP Endpoints Metrics",
  "uid": "http-endpoints-metrics",
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ]
}
